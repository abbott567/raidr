{
  "name": "raidr",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "node_modules/.bin/mocha -w"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "got": "^6.6.0",
    "marko": "^3.12.0",
    "mongoose": "^4.6.6",
    "morgan": "~1.6.1",
    "socket.io": "^1.5.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "mocha": "^3.1.2",
    "require-dir": "^0.3.1",
    "run-sequence": "^1.2.2",
    "xo": "^0.17.0",
    "zombie": "^5.0.4"
  },
  "xo": {
    "space": true,
    "ignores": [
      "public/**"
    ],
    "env": [
      "jquery",
      "browser"
    ],
    "rules": {
      "xo/no-process-exit": 0,
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
